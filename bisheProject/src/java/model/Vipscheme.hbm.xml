<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Vipscheme" table="vipscheme" schema="ssh">
        <id name="vschId">
            <column name="vsch_id" sql-type="tinyint(1)"/>
        </id>
        <property name="price">
            <column name="price" sql-type="int(8)"/>
        </property>
        <property name="give">
            <column name="give" sql-type="int(8)"/>
        </property>
        <property name="vschDesc">
            <column name="vsch_desc" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>